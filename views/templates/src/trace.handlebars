{{#if ip}}
<div class='space'>*</div>
<article class='ping_container'>
  <div>⟢ {{ip}}</div>
  {{#if orgname}}
  <div>{{orgname}}</div>
  {{else}} {{#if orgid}}
  <div>{{orgid}}</div>
  {{else}} {{#if netname}}
  <div>{{netname}}</div>
  {{/if}} {{/if}} {{/if}}
  <div>
    {{#if city}} {{city}}, {{/if}} {{#if country}} {{country}} {{/if}}
  </div>
  <a href='http://geohash.org/{{geohash}}' target=_blank>
    <div>{{geohash}}</div>
  </a>
</article>
{{#if cables}}
<div class='space'>*</div>
{{#each cables}}
<a href="{{this.url}}">
  <div class="cableContainer space">∿ {{this.name}} [{{this.owners}}]</div>
</a>
{{/each}} {{/if}} {{/if}}